{
  "name": "tslibtpl",
  "description": "A typescript lib template",
  "version": "0.1.0",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.5.3",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "lint": "eslint src/*.ts",
    "test": "jest",
    "build": "tsc"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "/src/.*\\.spec\\.ts$",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ]
  }
}
